package com;

public class Principal {
	public static void main(String[] args) {
		int i,j,k; 	// i=horas  j=minutos k=segundos
		
		// Simular el funcionamiento de un reloj
		for(i=0;i<1;i++) {
			for(j=0;j<60;j++) {
				for(k=0;k<60;k++) {
					System.out.println(i +":" + j + ":" + k);
				}
			}
			
		}
	}

}
